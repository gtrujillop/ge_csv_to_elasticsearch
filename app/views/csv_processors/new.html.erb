<div class="container">
  <div class="jumbotron">
    <div class="row">
    </div>
      <div class="col-lg-12">
        <% provide(:title, 'Cargar Eventos') %>
        <h1>Cargar Eventos</h1>

        <div class="row">
          <div class="col-md-6 col-md-offset-3">
            <%= form_for @processor, url: csv_processors_path, html: { multipart: true } do |f| %>

              <%= f.label :file, "Cargar archivo CSV" %>
              <%= f.file_field :file , accept: 'application/csv, text/csv, .csv'%>

              <%= f.submit "Cargar", class: "btn btn-primary" %>
            <% end %>
          </div>
        </div>
      </div>
  </div>
</div>
