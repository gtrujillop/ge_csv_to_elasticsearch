<div class="container">
  <div class="jumbotron">
    <div class="row">
    </div>
      <div class="col-lg-12">
        <% provide(:title, 'Cargar Eventos') %>
        <h1>Cargar Eventos</h1>

        <div class="row">
          <div class="col-md-6 col-md-offset-3">
            <%= form_for @record, url: csv_records_ges_path do |f| %>

              <%= f.label :file_loader, "Cargar archivo CSV" %>
              <%= f.file_field :events_csv , required: true, accept: :csv %>

              <%= f.submit "Cargar", class: "btn btn-primary" %>
            <% end %>
          </div>
        </div>
      </div>
  </div>
</div>
